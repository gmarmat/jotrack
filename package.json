{
  "name": "jotrack",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next node_modules/.cache .turbo && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache .turbo",
    "test": "echo 'Unit tests skipped due to tinypool path resolution issue. Run e2e tests with: npm run e2e' && exit 0",
    "test:vitest": "vitest run --reporter=basic",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "db:migrate": "tsx db/migrate.ts",
    "db:seed": "tsx db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "restore:apply": "node scripts/apply-restore.mjs",
    "release:tag": "git tag v0.2 -m 'v0.2' && git push origin v0.2",
    "eval:prompts": "tsx scripts/eval-prompts.ts"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@types/dompurify": "^3.0.5",
    "archiver": "^7.0.1",
    "better-sqlite3": "^11.0.0",
    "dompurify": "^3.2.7",
    "drizzle-orm": "^0.33.0",
    "extract-zip": "^2.0.1",
    "lucide-react": "^0.545.0",
    "mammoth": "^1.11.0",
    "marked": "^16.4.0",
    "next": "^14.2.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "swr": "^2.3.6",
    "uuid": "^10.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@types/archiver": "^6.0.3",
    "@types/better-sqlite3": "^7.6.11",
    "@types/extract-zip": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.19",
    "canvas": "^3.2.0",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.16.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wait-on": "^9.0.1"
  }
}
